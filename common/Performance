#!/sbin/sh and #!/sbin/prop
revert.Balanced
revert.Power save
# Do not modify after this
cpu.overclocked=yes
gpu.overclocked=yes
cpu.min.freq=1000Mhz
cpu.max.freq=2750Mhz
gpu.min.freq=500Mhz
if chip is Helio P60 then
do
gpu.max.freq=1000Mhz
if chip is Helio P70 then
do
gpu.max.freq=1100Mhz
display.resolution=1520x720
if touchpanel is Novatek then
do
restart.touchpanel
reset.touchpanel
and
change("MathRandompick /system/*, /vendor/* <Touchpanel>);
