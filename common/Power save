#!/sbin/sh and #!/sbin/prop
revert.Performance
revert.Balanced
# Do not modify after this
cpu.overclocked=no
cpu.min.freq=0Mhz
cpu.max.freq=2000Mhz
gpu.min.freq=100Mhz
if chip is Helio P60 then
do
gpu.max.freq=800Mhz
if chip is Helio P70 then
do
gpu.max.freq=900Mhz
display.resolution=1520x720
if touchpanel is Novatek then
do
restart.touchpanel
reset.touchpanel
and
change("MathRandompick /system/*, /vendor/* <Touchpanel>);
